<Window x:Class="game_model_remake.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:game_model_remake"
        mc:Ignorable="d"
        Title="Sokoban" Height="350" Width="525">
    <Grid>
        <Label x:Name="label" Content="Total Moves:" HorizontalAlignment="Left" Margin="203,0,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.029,0.577"/>
        <Button x:Name="btn_Left" Content="Left" HorizontalAlignment="Left" Margin="97,289,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Button x:Name="button" Content="Right" HorizontalAlignment="Left" Margin="177,289,0,0" VerticalAlignment="Top" Width="75" Click="button_Click_1"/>
        <Button x:Name="button1" Content="Up" HorizontalAlignment="Left" Margin="257,289,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"/>
        <Button x:Name="button2" Content="Down" HorizontalAlignment="Left" Margin="337,289,0,0" VerticalAlignment="Top" Width="75" Click="button2_Click"/>
        <Canvas x:Name="Canvas_Level" HorizontalAlignment="Left" Height="261" Margin="10,23,0,0" VerticalAlignment="Top" Width="497">
        </Canvas>
    </Grid>
</Window>
